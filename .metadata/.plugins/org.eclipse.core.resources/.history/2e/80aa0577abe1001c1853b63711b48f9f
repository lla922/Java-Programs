package bgm.models;

import java.util.List;

public class Support extends Champions {
	
	public SupportType type;
	private static List<Support> sups;

	public Support(String name,SupportType type) {
		super(name);
		this.type = type;
		// TODO Auto-generated constructor stub
	}
	
	public static List<Support> getSups() {
		return sups;
	}
	
	
	
	class SupportType {
		private String type;
		
		public void setType(String type) {
			this.type = type;
		}
	 
	 public SupportType(String type) {
		 this.type = type;
	 }
	 
	}

}
